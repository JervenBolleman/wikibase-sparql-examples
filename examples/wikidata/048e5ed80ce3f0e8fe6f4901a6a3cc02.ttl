	?item wdt:P31 wd:Q5 . 
  	?item wdt:P106 wd:Q1028181 .
	FILTER(CONTAINS(LCASE(?itemLabel), \"rembrandt\"))
	SERVICE wikibase:label { bd:serviceParam wikibase:language \"nl\" }    
}""";
