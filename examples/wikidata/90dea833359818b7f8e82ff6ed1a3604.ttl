<https://www.wikidata.org#query-90dea833359818b7f8e82ff6ed1a3604> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org/wiki/User:Simon_Villeneuve/PoV>;
