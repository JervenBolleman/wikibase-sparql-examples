<https://www.wikidata.org/#query-37fbbc040c08323af389aa00e1c4f2a6> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """# Things named after people born in cities that are part of Hungary
SELECT ?item ?itemLabel ?person ?personLabel ?coord
WHERE
{
    ?item wdt:P138 ?person .
    ?person wdt:P19 ?place .
    ?place wdt:P17 wd:Q28 .
    ?item wdt:P625 ?coord .

	SERVICE wikibase:label { 
      bd:serviceParam wikibase:language \"hu\" }
}""";
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:WikiProject_Hungary/Showcase_Queries>;
