<https://www.wikidata.org/#query-36eb1f0ee4b25e55e87a325c53be8ecd> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?Pays ?Ville ?Population
WHERE
{
  ?Ville wdt:P31/wdt:P279* wd:Q515;
        wdt:P17 ?Pays;
        wdt:P1082 ?Population.
}
ORDER BY ?Pays""";
