  SERVICE <https://query.wikidata.org/sparql> {
    ?item p:P1433 [ ps:P1433 wd:Q114151337; pq:P1545 ?id_ ] ;
      wdt:P18 ?image .
    BIND(xsd:integer(?id_) as ?id) .
  }
  ?file schema:url ?image ;
    schema:height ?height ;
    schema:width ?width .
}
ORDER BY (?width * ?height)""";
