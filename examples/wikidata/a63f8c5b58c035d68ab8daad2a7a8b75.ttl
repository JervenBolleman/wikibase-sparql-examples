  OPTIONAL { ?position_statement pq:P4100 ?faction_wikidata }
  OPTIONAL { ?position_statement pq:P582 ?mandate_end }
  BIND(COALESCE(?mandate_end, \"3001-01-01T00:00:00Z\"^^xsd:dateTime) as ?enddate)
  FILTER (?enddate > NOW())
}""";
