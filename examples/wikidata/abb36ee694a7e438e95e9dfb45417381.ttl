<https://www.wikidata.org/#query-abb36ee694a7e438e95e9dfb45417381> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#title:Ambassadors of Spain with position start & end dates & position ref
SELECT ?item ?itemLabel ?position ?positionLabel ?start ?end (GROUP_CONCAT(?ref;separator=\"; \") as ?refs)
WHERE 
{
  ?item p:P39 ?stat . 
  ?stat ps:P39 ?position .
  ?position wdt:P31 wd:Q6348379.
  OPTIONAL {?stat pq:P580 ?start .}
  OPTIONAL {?stat pq:P582 ?end .}
  OPTIONAL {?stat prov:wasDerivedFrom/pr:P4256 ?ref .}
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],es\". }
} GROUP BY ?item ?itemLabel ?position ?positionLabel ?start ?end""";
