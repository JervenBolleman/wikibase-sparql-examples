  }
  ?item rdfs:label ?label_de.
  filter (lang(?label_de) = \"de\").
  FILTER CONTAINS ( ?label_de, \"Voglerstraße\") 
  MINUS {
    ?item wdt:P669 []
  }
  BIND(concat(STR(?item),'##P669##Q63976279##P670##\"',?label_de,'\"') as ?qs)
} ORDER BY ?label_de""";
  schema:target <https://query.wikidata.org/sparql> .
