<https://www.wikidata.org/#query-3d0157da66a51865775e8e4cf8ffdabc> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?affiliationTypeLabel (COUNT(DISTINCT(?w)) AS ?number)
WHERE 
{
  ?p wdt:P31 wd:Q98270496 .
  ?p p:P1416 ?statement .
  ?statement ps:P1416 ?w .
  OPTIONAL { ?statement|| pq:P3831 ?affiliationType .}  
  OPTIONAL { ?p wdt:P571 ?inception }
  OPTIONAL { ?p wdt:P488 ?speaker }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"de,en\" . }
} GROUP BY ?affiliationTypeLabel""";
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Wiljes/NFDI>;
