<https://www.wikidata.org/#query-dff4f795a35ea1cdd6971ea57fa67dd3> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Synia:buriallocation>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
PREFIX target: <http://www.wikidata.org/entity/Q742670>

SELECT
  ?description ?descriptionLabel ?descriptionUrl

  ?value ?valueLabel ?valueUrl ?valueDescription
 
  ?wikidata ?wikidataUrl
WHERE {
  {
    BIND(1 AS ?order)
    BIND(wd:Q39614 AS ?description)
    BIND(\"#buriallocation\" AS ?descriptionUrl)
    BIND(target: AS ?value)
    BIND(\"Wikidata â†—\" AS ?wikidata)
    BIND(CONCAT(\"https://www.wikidata.org/wiki/\", SUBSTR(STR(?value), 32)) AS ?wikidataUrl)
  }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
} 
ORDER BY ?order""" .
