?q p:P195 ?s. ?s ps:P195 ?collection . MINUS {?s pq:P582 []}.
MINUS { ?s ps:P195 wd:Q1322278 }
?q wdt:P186 ?material
  SERVICE wikibase:label {
    bd:serviceParam wikibase:language \"en\".
    ?material rdfs:label ?l; schema:description ?d
  }
}
GROUP BY ?material ?l ?d""";
