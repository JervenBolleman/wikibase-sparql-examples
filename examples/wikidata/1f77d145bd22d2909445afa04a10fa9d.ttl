<https://www.wikidata.org/#query-1f77d145bd22d2909445afa04a10fa9d> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """# Could anyone please provide a query that lists items with P91 statements, which are P143-“sourced”

select ?item where {
  ?item p:P91 [
    prov:wasDerivedFrom ?ref
  ] .
  ?ref pr:P143 []
  filter not exists { ?ref ?prop [] . filter (?prop != pr:P143)} .
}""";
