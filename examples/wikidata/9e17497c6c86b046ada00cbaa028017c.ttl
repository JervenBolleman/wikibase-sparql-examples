@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-9e17497c6c86b046ada00cbaa028017c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Rdrg109/1/13>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX ontolex: <http://www.w3.org/ns/lemon/ontolex#>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT
  ?language
  ?languageLabel
  ?total
  ?without
  ?with
  ?ratio
{
  INCLUDE %1.
  INCLUDE %2.
  BIND((?total - ?with) AS ?without)
  BIND(CONCAT(STR((?with/?total)), \" %\") AS ?ratio)
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}
ORDER BY DESC(?without)""";
  <http://example.org/bigdata_select> """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX ontolex: <http://www.w3.org/ns/lemon/ontolex#>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT
  ?language
  ?languageLabel
  ?total
  ?without
  ?with
  ?ratio
WITH {
  SELECT ?language {
    VALUES ?language {
      wd:Q1321
    }
  }
} AS %0
WITH {
  SELECT ?language (COUNT(*) AS ?with) {
   INCLUDE %0.
    [] dct:language ?language;
       ontolex:lexicalForm [wdt:P443 []]
  }
  GROUP BY ?language
} AS %1
WITH {
  SELECT ?language (COUNT(*) AS ?total) {
    INCLUDE %0.
    [] dct:language ?language;
       ontolex:lexicalForm [].
  }
  GROUP BY ?language
} AS %2
{
  INCLUDE %1.
  INCLUDE %2.
  BIND((?total - ?with) AS ?without)
  BIND(CONCAT(STR((?with/?total)), \" %\") AS ?ratio)
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}
ORDER BY DESC(?without)""" .
