<https://www.wikidata.org/#query-704a5d5ae5d8883d6c8732cff23c93ff> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User_talk:BambooBeast>;
