<https://www.wikidata.org/#query-b631a1e5523ea6eacb44357014fc6470> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:WikiProject_Government/Report/Jamaica>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#title: Query to retrieve the Ministries of Jamaica (should be 17)
SELECT DISTINCT ?item ?itemLabel WHERE {
  ?item wdt:P31 wd:Q127413860; #instance of Ministry of Jamaica
        wdt:P17 wd:Q766. #country Jamaica
  MINUS { ?item wdt:P576 [] } #dissolved or abolished
  MINUS { ?item wdt:P1366 [] } #replaced by
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY ?itemLabel""" .
