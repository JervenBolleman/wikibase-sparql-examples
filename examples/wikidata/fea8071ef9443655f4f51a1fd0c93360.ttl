    FILTER(LANG(?itemDesc) = \"en\")
  }
  BIND (ENCODE_FOR_URI(REPLACE(STR(?item), \".*Q\", \"Q\")) AS ?qid)
  BIND (YEAR(?pubdate) AS ?pubyear)
  BIND (CONCAT(\"scientific article published in \", STR(?pubyear)) AS ?Den)
}""";
