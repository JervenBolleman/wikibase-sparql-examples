<https://www.wikidata.org/#query-a7630454fdf7f0f44c17f7317fdd4919> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Singlespeedfahrer/sparql-requests>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#title:Karte von Skulpturen von Ingeborg Hunzinger
#defaultView:Map{\"hide\":[\"?coord\"]}
SELECT DISTINCT ?id  ?item ?itemLabel ?itemDescription ?img ?coord   WHERE {
      ?item p:P31 ?statement0.
      ?statement0 (ps:P31/(wdt:P279*)) wd:Q860861.
      ?item p:P170 ?statement1.
      ?statement1 (ps:P170/(wdt:P279*)) wd:Q291173.
  
  SERVICE wikibase:label {
    bd:serviceParam wikibase:language \"de,en\" . 
  }
  
  OPTIONAL { ?item wdt:P217 ?id. }
  OPTIONAL { ?item wdt:P18 ?img. }
  OPTIONAL { ?item wdt:P625 ?coord. }
}""" .
