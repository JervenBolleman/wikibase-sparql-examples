<https://www.wikidata.org/#query-d5b50df23a2b3a2444966d8e4fd73fed> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel ?person ?personLabel ?start ?end
WHERE 
{
  VALUES ?item {wd:Q231270}
  ?item p:P451 ?stat . 
  ?stat ps:P451 ?person .
  OPTIONAL {?stat pq:P580 ?start . }
  OPTIONAL {?stat pq:P582 ?end . }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". } 
}""";
