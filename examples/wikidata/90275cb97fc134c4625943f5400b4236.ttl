WHERE 
{
  ?item wdt:P106/wdt:P279* wd:Q36180.                    #writer
  ?item skos:altLabel ?alias. filter(lang(?alias)=\"en\")
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
