    OPTIONAL [[:Template:?ind rdfs:label ?indLabel . FILTER ( lang(?indLabel) = 'en' )]]
    OPTIONAL [[:Template:?i rdfs:label ?iLabel . FILTER ( lang(?iLabel) = 'en' )]]
}} GROUP BY ?ind ?indLabel""";
