<https://www.wikidata.org/#query-f22d4ea3ca6ad36e34443f0c522c80eb> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel
WHERE
{
  ?item wdt:P31 / wdt:P279 * wd:Q159719 . # ?item is a powerplant
  ?item wdt:P17 wd:Q43 . # ?item is in Turkey
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
