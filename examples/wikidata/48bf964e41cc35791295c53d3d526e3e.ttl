<https://www.wikidata.org/#query-48bf964e41cc35791295c53d3d526e3e> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Pere_prlpz>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
#Fitxers de Commons que representen alguna masia
SELECT ?file
WHERE {
  
{
  SELECT ?item 
  WHERE
  {
    SERVICE <https://query.wikidata.org/sparql>
    {
        ?item wdt:P31 wd:Q585956.
     } 
  }
}  ?file wdt:P180 ?item.
}
LIMIT 1000""";
  <http://example.org/bigdata_select> """#Fitxers de Commons que representen alguna masia
SELECT ?file
WITH
{
  SELECT ?item 
  WHERE
  {
    SERVICE <https://query.wikidata.org/sparql>
    {
        ?item wdt:P31 wd:Q585956.
     } 
  }
} AS %get_items
WHERE {
  INCLUDE %get_items
  ?file wdt:P180 ?item.
}
LIMIT 1000""" .
