  MINUS { ?airport wdt:P576 _:b2. }
  MINUS { ?airport wdt:P582 _:b3. }
  OPTIONAL { ?airport wdt:P625 ?coords. }
  OPTIONAL { ?airport wdt:P17 ?pays. }
  MINUS { ?airport wdt:P30 wd:Q51. }
FILTER(!BOUND(?pays))
}""";
