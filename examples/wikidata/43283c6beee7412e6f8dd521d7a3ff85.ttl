<https://www.wikidata.org/#query-43283c6beee7412e6f8dd521d7a3ff85> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?Member ?MemberLabel ?EUpartyLabel ?NationalPartyLabel ?countryLabel WHERE {
  ?Member p:P39 ?ps .
  ?ps ps:P39 wd:Q27169 ; pq:P2937 wd:Q64038205.
  OPTIONAL {?ps pq:P4100 ?EUparty.}
  OPTIONAL {?ps pq:P1268 ?NationalParty.}
  OPTIONAL {?ps pq:P768 ?country.}   
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE]\". }
}""";
