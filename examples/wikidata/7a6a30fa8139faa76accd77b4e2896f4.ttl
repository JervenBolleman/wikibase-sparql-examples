<https://www.wikidata.org/#query-7a6a30fa8139faa76accd77b4e2896f4> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel (group_concat(distinct ?platform;separator=\", \") as ?platforms) (group_concat(distinct ?genre;separator=\", \") as ?genres) with {
  SELECT ?item WHERE 
  {
    ?item wdt:P31 wd:Q7889 .
  } LIMIT 10 } as %i
WHERE
{ 
  include %i
  ?item wdt:P400 ?platform.
  ?item wdt:P136 ?genre.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en,en\". }
} group by ?item ?itemLabel""";
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2019/02>;
