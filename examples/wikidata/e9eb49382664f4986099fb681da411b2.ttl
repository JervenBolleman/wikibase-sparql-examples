}
}  ?literary_work wdt:P31/wdt:P279* wd:Q386724 .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"it,en\". }
}
ORDER BY ?awardLabel ?year ?STR(?literary_workLabel)""";
