SELECT DISTINCT ?awardLabel (YEAR(?p585) AS ?year) ?literary_work ?literary_workLabel ?authorLabel  WHERE {
   {
SELECT ?literary_work ?award ?author ?p585 WHERE {
  ?award wdt:P31/wdt:P279* wd:Q378427 .
