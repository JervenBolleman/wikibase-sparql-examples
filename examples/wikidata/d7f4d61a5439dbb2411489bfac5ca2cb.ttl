  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
PREFIX pq: <http://www.wikidata.org/prop/qualifier/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT DISTINCT ?country ?countryLabel ?head ?headLabel ?office ?officeLabel ?officeholder ?officeholderLabel ?person ?personLabel   
WHERE {      
  ?country wdt:P31 wd:Q3624078 ; # find all countries
           wdt:P1313 ?office . # and the office held by head of government
  FILTER NOT EXISTS { ?office wdt:P576 ?end_date } . # (non-dissolved)
  FILTER (?country != wd:Q238) # ignoring San-Marino's co-heads

  OPTIONAL {
    ?person p:P39 ?posn ; wdt:P31 wd:Q5 .  # find all humans with a position
    ?posn ps:P39 ?office ;                 # of that office
          pq:P580 ?start_date .            # where the position statement has a start date
          FILTER (?start_date < NOW()) .   #   but not in the future

    OPTIONAL { ?posn pq:P582 ?end_date }  
    FILTER ( !bound(?end_date) || isBLANK(?end_date) ) . # but with no end date
  }
                                                                                          
  OPTIONAL { ?country wdt:P6 ?head }  # and compare with the Head of Government on the country
  OPTIONAL { ?office wdt:P1308 ?officeholder } # and the holder of the HoG office
  OPTIONAL { ?country wdt:P1082 ?population }
  
  FILTER ((?officeholder != ?head) || (?head != ?person) )
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\" . }
}
ORDER BY DESC(?population)""";
