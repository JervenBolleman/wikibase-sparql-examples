PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#defaultView:BarChart
select ?genderLabel (count(?gender) as ?genderCount) where {
  ?item wdt:P136 wd:Q557141;
        wdt:P131 wd:Q47887;
        wdt:P170 ?creator. 
    
  ?creator wdt:P21 ?gender.
