<https://www.wikidata.org#query-6ee7faa94231361ef79676a620c73667> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org/wiki/User:Pmartinolli/UdeM_SPARQL>;
