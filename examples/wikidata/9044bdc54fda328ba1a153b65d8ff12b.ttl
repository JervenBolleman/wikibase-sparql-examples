<https://www.wikidata.org#query-9044bdc54fda328ba1a153b65d8ff12b> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org/wiki/User:MartinPoulter/queries>;
