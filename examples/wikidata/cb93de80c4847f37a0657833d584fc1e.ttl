<https://www.wikidata.org/#query-cb93de80c4847f37a0657833d584fc1e> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2022/02>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX pq: <http://www.wikidata.org/prop/qualifier/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?item ?itemLabel ?statement ?prop ?propLabel ?valueLabel ?forLabel
WHERE
{
  
{
  SELECT ?item ?claim ?statement ?for
  WHERE
  {
    ?item ?claim ?statement .
    ?statement pq:P642 ?for .
    FILTER (?claim != p:P31 && ?claim != p:P279 && ?claim != p:P2699)
  }
  LIMIT 1000
}  ?prop wikibase:claim ?claim .
  ?prop wikibase:statementProperty ?sp .
  ?statement ?sp ?value .
  SERVICE wikibase:label {
    bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\" .
  }
}""";
  <http://example.org/bigdata_select> """SELECT ?item ?itemLabel ?statement ?prop ?propLabel ?valueLabel ?forLabel
WITH
{
  SELECT ?item ?claim ?statement ?for
  WHERE
  {
    ?item ?claim ?statement .
    ?statement pq:P642 ?for .
    FILTER (?claim != p:P31 && ?claim != p:P279 && ?claim != p:P2699)
  }
  LIMIT 1000
} AS %query
WHERE
{
  INCLUDE %query
  ?prop wikibase:claim ?claim .
  ?prop wikibase:statementProperty ?sp .
  ?statement ?sp ?value .
  SERVICE wikibase:label {
    bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\" .
  }
}""" .
