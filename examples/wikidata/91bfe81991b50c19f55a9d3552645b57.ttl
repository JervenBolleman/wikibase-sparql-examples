@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-91bfe81991b50c19f55a9d3552645b57> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?language ?languageLabel (COUNT(?language) AS ?count)
WHERE
{
  ?country wdt:P31 wd:Q6256.
  ?country wdt:P37 ?language.
  ?language wdt:P282 ?script.

  { ?script wdt:P279* wd:Q8229. } # The script is a subclass of Latin script.
  UNION
  { ?script wdt:P31 wd:Q29575627 }

  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
GROUP BY ?language ?languageLabel
ORDER BY DESC(?count)""";
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2020/04>;
