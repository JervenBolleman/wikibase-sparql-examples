    OPTIONAL {?item rdfs:label ?en_label . FILTER(LANG(?en_label) = \"en\")}
    OPTIONAL {?item rdfs:label ?item_label}
    #?item wdt:P17 wd:Q794.
  }
  GROUP BY ?item ?en_label
}""";
