  ?uni wdt:P17 wd:Q183 .    # nur Deutsche Unis
  ?uni wdt:P2196 ?numberOfStudents .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],de,en\" }
}
ORDER BY DESC(?numberOfStudents)""";
