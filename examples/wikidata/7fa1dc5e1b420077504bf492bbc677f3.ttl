<https://www.wikidata.org#query-7fa1dc5e1b420077504bf492bbc677f3> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org/wiki/User:Stefan_K%C3%BChn/Persondata>;
