<https://www.wikidata.org/#query-795ab9abc54650df3a70e9c447424523> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#List all items containing a statement using property:P35
SELECT ?item ?itemLabel 
WHERE 
{
  ?item wdt:P35 ?anything
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:SPARQL_query_service/queries/examples/sandbox>;
