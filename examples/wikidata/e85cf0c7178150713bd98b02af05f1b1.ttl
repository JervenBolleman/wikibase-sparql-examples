    p:P27 ?statement.
  FILTER NOT EXISTS { ?statement pq:P580 ?datainizio. }
  FILTER(\"1946-06-18\"^^xsd:dateTime > ?nato)
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],it,en\". }
