<https://www.wikidata.org/#query-7078b8ca9844d62c91dc58c50818b56e> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#shows all collections which have a work by a specific artist
SELECT ?work ?workLabel ?collection ?collectionLabel WHERE {
  ?work wdt:P170 wd:Q2519247.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"nl\". }
  OPTIONAL { ?work wdt:P195 ?collection. }
}""";
