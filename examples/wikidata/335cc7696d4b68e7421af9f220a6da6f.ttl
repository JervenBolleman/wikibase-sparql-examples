  rdfs:comment "Propertiessubclass of (P279)ISO standard (P503)publication date (P577)instance of (P31)"@en;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT DISTINCT ?format ?formatLabel ?standardLabel ?date
WHERE 
{
  
  ?format  (p:P31/ps:P31/wdt:P279*) wd:Q235557 .
  ?format wdt:P503 ?standard.
  OPTIONAL {?format wdt:P577 ?date.}
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
