    wd:Q57577442   # Feldweg
    wd:Q207934     # Allee
    wd:Q628179     # Weg
    wd:Q5004679    # Weg
    wd:Q221722     # Radweg
    wd:Q787113     # Promenade
    wd:Q1320830    # Ladenpassage
    } 
  ?Objekt wdt:P31 ?instance_of;
          wdt:P8138 wd:Q5649223 .
  OPTIONAL { ?Objekt wdt:P138 ?Namensgeber . }
}
ORDER BY ASC ( ?ObjektLabel )""";
