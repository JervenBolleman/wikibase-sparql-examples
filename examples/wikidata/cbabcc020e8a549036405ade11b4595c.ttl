<https://www.wikidata.org/#query-cbabcc020e8a549036405ade11b4595c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#works
SELECT ?item
WHERE
{ 
        ?item wdt:P31 wd:Q5
        OPTIONAL { ?item schema:description ?descnl FILTER(LANG(?descnl) = \"nl\")   }
        FILTER ( !BOUND(?descnl) )
}
LIMIT 5000""";
