<https://www.wikidata.org/#query-f301cd45e1d7220002dbbfddc8511499> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?reg ?regLabel ?cap ?capLabel WHERE {
  # region of Finland
  ?reg wdt:P31 wd:Q193512 .
  
  # find the capital
  ?reg wdt:P36 ?cap .
  
  SERVICE wikibase:label {
     bd:serviceParam wikibase:language \"fi\" .
  }
} ORDER BY ?regLabel""";
