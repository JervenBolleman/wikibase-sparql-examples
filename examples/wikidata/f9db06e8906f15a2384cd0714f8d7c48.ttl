<https://www.wikidata.org/#query-f9db06e8906f15a2384cd0714f8d7c48> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?taxonname ?authLabel ?year
WHERE
{
     ?tephs wdt:P171+ wd:Q626843. 
     ?tephs p:P225 ?name.
     ?name  ps:P225 ?taxonname.
     ?name  pq:P405 ?auth.
     ?name  pq:P574 ?year.
     SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}
ORDER BY ?authLabel""";
