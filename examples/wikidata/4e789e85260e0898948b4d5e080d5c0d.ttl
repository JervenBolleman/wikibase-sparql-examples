                     mwapi:gcmtitle \"Category:Humanitarian aid\" ;         # category
                     mwapi:gcmprop \"title\";
                     mwapi:gcmlimit \"max\".
     ?article_name wikibase:apiOutput mwapi:title.        
     ?item wikibase:apiOutputItem mwapi:item.     
     ?ns wikibase:apiOutput \"@ns\".
     }
  filter(BOUND(?item))
