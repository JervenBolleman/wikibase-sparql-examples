<https://www.wikidata.org/#query-9a383d1fee36538caff3ba9cbf50eeb3> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#defaultView:BarChart
SELECT ?surname ?surnameLabel ?count WHERE {
  {
    SELECT ?surname (COUNT(?person) AS ?count) WHERE {
      ?person wdt:P31 wd:Q5.
      ?person wdt:P734 ?surname.
      ?surname wdt:P407 wd:Q7918.
    }
    GROUP BY ?surname
  }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY DESC(?count)""";
  rdfs:comment "TODO"@en;
