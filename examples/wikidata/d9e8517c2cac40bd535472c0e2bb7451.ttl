<https://www.wikidata.org#query-d9e8517c2cac40bd535472c0e2bb7451> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org/wiki/User:Bouzinac/Recherches_d%27a%C3%A9roports>;
