<https://www.wikidata.org/#query-8a8d731a9cf1b12731895d8c6199d7b4> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#defaultView:BarChart
SELECT  (sample(?uniLabel) as ?label) (count(?uni) as ?count)  WHERE {
  ?item wdt:P39 wd:Q28778415.
  ?item p:P39 ?elect.
  ?elect ps:P39 ?elect_ .
  ?elect pq:P2937 ?term .
  ?term wdt:P31 wd:Q106450751.
             ?term wdt:P580 ?start.
  ?item wdt:P69 ?uni.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". 
                         ?uni rdfs:label ?uniLabel.}
}
GROUP by ?uniLabel ?start""";
