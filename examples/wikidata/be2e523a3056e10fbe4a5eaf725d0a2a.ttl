      MINUS { ?school (wdt:P17|wdt:P131)* wd:Q1246 } .
      #?school wdt:P17 wd:Q183 . # no results
    } GROUP BY ?item
  } .
  OPTIONAL { ?school wdt:P17 ?country } .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"sq,sr,en\" } .
}""";
