 ?item rdfs:label ?label filter (lang(?label) = \"it\")
 ?item wdt:P407 wd:Q652.
 ?article schema:about ?item ;
 #schema:inLanguage ?lang ;
# schema:isPartOf [ wikibase:wikiGroup \"wikipedia\" ] .

} group by ?item ?label ?labelgen""";
