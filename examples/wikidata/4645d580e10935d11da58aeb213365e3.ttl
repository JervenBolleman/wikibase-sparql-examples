  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#defaultView:Table
SELECT ?count ?author ?authorLabel ?sample_pub ?sample_pubLabel
WHERE {
   {
  SELECT (count(DISTINCT ?work) as ?count) ?author (SAMPLE(?work) AS ?sample_pub) WHERE {
    ?work wdt:P6104 wd:Q56241615 .
    ?work wdt:P921 ?topic . 
    ?work wdt:P50 ?author .
  }
  GROUP BY ?author
  ORDER BY DESC(?count)
  LIMIT 200
}  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTOLANG],en\" . } 
}
ORDER BY DESC(?count)""";
  <http://example.org/bigdata_select> """#defaultView:Table
SELECT ?count ?author ?authorLabel ?sample_pub ?sample_pubLabel
WITH {
  SELECT (count(DISTINCT ?work) as ?count) ?author (SAMPLE(?work) AS ?sample_pub) WHERE {
    ?work wdt:P6104 wd:Q56241615 .
    ?work wdt:P921 ?topic . 
    ?work wdt:P50 ?author .
  }
  GROUP BY ?author
  ORDER BY DESC(?count)
  LIMIT 200
} AS %result
WHERE {
  INCLUDE %result
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTOLANG],en\" . } 
}
ORDER BY DESC(?count)""";
