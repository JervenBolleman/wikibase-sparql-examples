  OPTIONAL {?item wdt:P1619 ?dateOfOpening ; wdt:P3999 ?dateOfClosure  } .
  OPTIONAL {?item wdt:P127 ?ownedBy}.
 }
UNION
 {
  {VALUES (?itemType) {(wd:Q11707) (wd:Q30022)(wd:Q622425) }
   ?item wdt:P31/wdt:P279* ?itemType .
   ?item wdt:P5008 wd:Q100202113 .
