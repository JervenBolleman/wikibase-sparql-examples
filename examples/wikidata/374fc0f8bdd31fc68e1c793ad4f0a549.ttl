SELECT ?indice_desarrollo ?esperanza_vida ?paisLabel ?pais  WHERE {
  ?pais wdt:P463 wd:Q458 .
  ?pais wdt:P1081 ?indice_desarrollo .
  ?pais wdt:P2250 ?esperanza_vida .
