<https://www.wikidata.org/#query-73552eb60a8951db73fefec099623c85> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT 
  (?awardLabel AS ?Award_participant_in)
  (YEAR(?awardYear) AS ?Award_year)
  (?project AS ?Project_Wd_item_link)
  (?projectLabel AS ?Project_name)  
WHERE
{
  ?project p:P1344 ?awardStatement.
  ?awardStatement ps:P1344 ?award.
  ?award wdt:P361 wd:Q106748908.
  ?awardStatement pq:P585 ?awardYear.  
  SERVICE wikibase:label {
    bd:serviceParam wikibase:language \"de,en\".
    ?project rdfs:label ?projectLabel.
    ?award rdfs:label ?awardLabel.
  }
}
ORDER BY ?awardLabel ?awardYear ?projectLabel""";
