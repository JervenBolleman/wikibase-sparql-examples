      ?url schema:isPartOf <https://commons.wikimedia.org/> .
      FILTER( REGEX(str(?url), \"^https://commons\\\\.wikimedia\\\\.org/wiki/Institution:\") )
    }
    ?url schema:about ?item .
}
LIMIT 100""";
