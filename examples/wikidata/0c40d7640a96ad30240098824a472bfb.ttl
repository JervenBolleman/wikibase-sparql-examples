  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#defaultView:Tree
SELECT ?root ?rootLabel 
        ?level1 ?level1Label 
        ?level2 ?level2Label  
        ?level3 ?level3Label 
        ?level4 ?level4Label 
        ?level5 ?level5Label 
        ?level6 ?level6Label 
        ?level7 ?level7Label 
        ?level8 ?level8Label 
        ?level9 ?level9Label 
  WHERE {
  BIND(wd:Q2824642 AS ?super)
  ?level1 wdt:P279 ?super.
  OPTIONAL {
    ?level2 wdt:P279 ?level1. 
    OPTIONAL {
      ?level3 wdt:P279 ?level2.
          OPTIONAL {
            ?level4 wdt:P279 ?level3.
            OPTIONAL {
              ?level5 wdt:P279 ?level4.
              OPTIONAL {
                ?level6 wdt:P279 ?level5.
                OPTIONAL {
                  ?level7 wdt:P279 ?level6.
                  OPTIONAL {
                    ?level8 wdt:P279 ?level7.
                      OPTIONAL {
                      ?level9 wdt:P279 ?level8.
                     }
                   }
                 }
               }
             }
          }
      }
    }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"fr\". }
}""";
