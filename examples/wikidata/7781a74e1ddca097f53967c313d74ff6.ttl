<https://www.wikidata.org/#query-7781a74e1ddca097f53967c313d74ff6> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Japastorsanchez/BID-SPARQL>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?universe ?universeLabel ?num_universe (count(?work) as ?num_works) 
WHERE {
   {
   SELECT DISTINCT ?universe (count(?item) as ?num_universe) WHERE {
      ?item wdt:P1080 ?universe .
   } GROUP BY ?universe
}
   OPTIONAL {?work wdt:P1434 ?universe .}
   OPTIONAL {?universe wdt:P1445 ?work .}
   SERVICE wikibase:label {bd:serviceParam wikibase:language \"es\",\"en\" .}
} GROUP BY ?universe ?universeLabel ?num_universe""";
  <http://example.org/bigdata_select> """SELECT ?universe ?universeLabel ?num_universe (count(?work) as ?num_works) WITH {
   SELECT DISTINCT ?universe (count(?item) as ?num_universe) WHERE {
      ?item wdt:P1080 ?universe .
   } GROUP BY ?universe
} AS %numitems
WHERE {
   INCLUDE %numitems
   OPTIONAL {?work wdt:P1434 ?universe .}
   OPTIONAL {?universe wdt:P1445 ?work .}
   SERVICE wikibase:label {bd:serviceParam wikibase:language \"es\",\"en\" .}
} GROUP BY ?universe ?universeLabel ?num_universe""",
