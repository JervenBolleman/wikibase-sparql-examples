<https://www.wikidata.org/#query-f95315b2cf1e0e851077096d5f12838f> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel ?shortName WHERE {  
  ?item wdt:P31 wd:Q64062850 .  
  OPTIONAL {?item wdt:P1813 ?shortName} .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE]\". }
}
ORDER BY ASC(?shortName)""";
