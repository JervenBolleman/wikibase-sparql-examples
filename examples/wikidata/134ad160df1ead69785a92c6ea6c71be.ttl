#title: Kategorie Olympische Sommerspiele 1972 und Unterkategorien
SELECT ?kategorie ?member ?item ?article (GROUP_CONCAT(DISTINCT ?class; SEPARATOR = \", \") AS ?ist_ein) ?location WHERE { 
