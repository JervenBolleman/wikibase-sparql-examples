    OPTIONAL { ?item wdt:P625 ?location. }
    SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". 
            ?instance rdfs:label ?class.} 
}
group by ?kategorie ?member ?item ?article ?location 
