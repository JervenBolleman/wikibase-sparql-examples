<https://www.wikidata.org/#query-61ca1834bc2d3a44ea5ccdb5e913b714> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?item WHERE { 
  {?item wdt:P17 wd:Q142 }
  UNION  {?item wdt:P27 wd:Q142 }
  ?sitelink schema:about ?item . 
FILTER NOT EXISTS { ?wfr schema:about ?item . ?wfr schema:inLanguage \"he\" } .
OPTIONAL {?item rdfs:label ?label1 filter(lang(?label1) = \"he\")}.
FILTER BOUND (?label1).
}LIMIT 800""";
