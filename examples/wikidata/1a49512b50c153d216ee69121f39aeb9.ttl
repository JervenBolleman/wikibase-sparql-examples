<https://www.wikidata.org/#query-1a49512b50c153d216ee69121f39aeb9> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Forum/Archiv/2023/02>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
PREFIX pq: <http://www.wikidata.org/prop/qualifier/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?item ?typ ?GKZ ?bürgermeisterLabel
WHERE {
        { ?item wdt:P131* wd:Q980. }             # in Bayern
  OPTIONAL { ?item p:P6 ?statement .
             ?statement ps:P6 ?bürgermeister .
             ?statement pq:P580 ?start .
           }
   { ?item wdt:P439 ?GKZ . }    # Gemeindekennzahl
   { ?item wdt:P31 ?typ . }    # ist ein
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"de\" }
}""";
  <https://purl.expasy.org/sparql-examples/ontology#federatesWith> <http://wikiba.se/ontology#label> .
