SELECT DISTINCT ?item ?itemLabel (xsd:decimal(REPLACE(str(?itemLabel),\"route impériale \",\"\")) as ?num) ?num_rn {
?item_rn (wdt:P31|wdt:P16) wd:Q1426271 ; wdt:P1365 ?item ; wdt:P1824 ?rn .
