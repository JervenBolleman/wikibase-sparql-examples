<https://www.wikidata.org/#query-ead99444b8ec57a71e024573e2499abc> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?city ?cityLabel
WHERE {
  #VALUES ?city { wd:Q64}
  ?city wdt:P31+ wd:Q5119.
  #wd:Q183 wdt:P36 ?city.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}""";
