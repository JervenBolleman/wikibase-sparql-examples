 bind( if( bound(?kdm) && strStarts(str(?kdm), \"I \"), CONCAT(STR( ?kdm  ), \".5\"), 
       if( bound(?next),                              CONCAT(STR( ?next ), \".9\"),
       if( bound(?prev),                              CONCAT(STR( ?prev ), \".1\"), 
       \"Z\") ) ) as ?kdmn ) .

 OPTIONAL {
