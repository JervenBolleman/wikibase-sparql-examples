#title:Allievi della Scuola Normale Superiore con eventuale SBN
SELECT ?persona ?personaLabel ?personaDescription ?sbn
WHERE {
  ?persona p:P69 ?st . ?st ps:P69 ?classe . ?classe wdt:P361 wd:Q672416 .
