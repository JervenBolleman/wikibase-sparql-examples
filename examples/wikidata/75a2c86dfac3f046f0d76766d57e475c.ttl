<https://www.wikidata.org/#query-75a2c86dfac3f046f0d76766d57e475c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#defaultView:Map
SELECT DISTINCT ?id ?idLabel ?location ?image ?link
WHERE {
    ?id wdt:P4614/wdt:P361* wd:Q48983740 . # Bassin du fleuve Saint-Laurent
    ?id wdt:P31/wdt:P279* wd:Q355304 .
    ?id wdt:P625 ?location .
    OPTIONAL {?id wdt:P18 ?image }
    SERVICE wikibase:label {bd:serviceParam wikibase:language \"fr\" . }
    OPTIONAL {
            ?link schema:about ?id .
            ?link schema:isPartOf <https://fr.wikipedia.org/> .
   }
}""";
