<https://www.wikidata.org#query-fe8d5122a6d1d3ed3a4dafb4d4853db1> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org/wiki/User:YULdigitalpreservation/SPARQL2meta>;
