<https://www.wikidata.org#query-816fc51b7ba175bc9a15b1c4fef620c8> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org/wiki/User:D1gggg/Wikidata_model_and_SPARQL>;
