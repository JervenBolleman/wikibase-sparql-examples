<https://www.wikidata.org/#query-ca7453f81c5ca269f6b37befc7bb8dc4> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?member ?id ?partyID ?constituencyID WHERE {
      ?member p:P39 ?ps .
      ?ps ps:P39 wd:Q30524710 .
      FILTER NOT EXISTS { ?ps pq:P582 [] }
      OPTIONAL { ?member wdt:P6213 ?id }
      OPTIONAL { 
        ?ps pq:P768 ?constituency .
        OPTIONAL { ?constituency wdt:P6213 ?constituencyID }
      }  
      OPTIONAL { 
        ?ps pq:P4100 ?party .
        OPTIONAL { ?party wdt:P6213 ?partyID }
      }
      SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
    }""";
