@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-fae8614fcac7452e4286c0e19a3e4f8c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#Autors de fitxers de Commons que representen alguna masia
SELECT ?file ?username
WITH
{
  SELECT ?item 
  WHERE
  {
    SERVICE <https://query.wikidata.org/sparql>
    {
        ?item wdt:P31 wd:Q585956.
     } 
  }
} AS %get_items
WHERE {
  INCLUDE %get_items
  ?file wdt:P180 ?item.
  ?file (p:P170/pq:P4174) ?username.
}
LIMIT 1000""";
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Pere_prlpz>;
