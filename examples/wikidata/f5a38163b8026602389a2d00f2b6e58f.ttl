        wdt:P144 ?novel; 
        wdt:P577 ?filmPubDate .
  ?novel wdt:P31/wdt:P279* wd:Q571; 
         wdt:P577 ?bookPubDate ;
         wdt:P179 wd:Q8337 .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}
ORDER BY ?bookYear ?filmYear""";
