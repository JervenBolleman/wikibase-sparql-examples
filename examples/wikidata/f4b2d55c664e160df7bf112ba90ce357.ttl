  BIND(REPLACE(?x,\"XYZ\",ENCODE_FOR_URI(?name)) AS ?url).  
  BIND(URI(CONCAT(?url, \"&hl=en\")) as ?en)
  BIND(URI(CONCAT(?url, \"&hl=fr\")) as ?fr) 
  }""";
