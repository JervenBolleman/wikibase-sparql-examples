  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX bd: <http://www.bigdata.com/rdf#>
# Abfrage Schweizer Kulturgut von regionaler Bedeutung (B-Objekte) mit EGID-Nummer
SELECT DISTINCT ?item ?itemLabel ?itemDescription 
WHERE
{
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE]\". }
  {
    SELECT DISTINCT ?item
    WHERE 
    {
      ?item p:P381 ?statement0.
      ?statement0 (ps:P381) _:anyValueP381.
      ?item p:P1435 ?statement1.
      ?statement1 (ps:P1435) wd:Q12126757. # (Schutzkategorie) Schweizer Kulturgut von regionaler Bedeutung (B-Objekte)
      ?item p:P9665 ?statement2.
      ?statement2 (ps:P9665) _:anyValueP9665. # Identifikator eines Gebäudes im Eidgenössischen Gebäude- und Wohnungsregister der Schweiz
    }
    LIMIT 10500 # Gemäss BABS: \"...rund 10'000 B-Objekte\"
  }
}""";
