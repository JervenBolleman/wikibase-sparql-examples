# Thèses de l'Université de Montréal
SELECT DISTINCT ?entity ?entityLabel ?value ?valueLabel 

WHERE {  

?entity wdt:P31/wdt:P279? wd:Q1266946 .  
?entity wdt:P4101 wd:Q392189 .  
?entity p:P1476 ?prop . 
