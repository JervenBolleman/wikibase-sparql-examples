<https://www.wikidata.org/#query-a9e6cbf12eb48e53b12461e318845567> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?workLabel ?work ?authorLabel ?author ?isniID
WHERE {
  ?work wdt:P50 ?author;
        rdfs:label ?workLabel.
  ?author wdt:P213 ?isniID.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\" }
}
LIMIT 100""";
