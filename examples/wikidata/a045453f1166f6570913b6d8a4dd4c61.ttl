<https://www.wikidata.org/#query-a045453f1166f6570913b6d8a4dd4c61> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """# Elenco di drammaturghi italiani nati nel 900 in ordine di data di nascita
SELECT ?item ?itemLabel ?ddn ?cc WHERE {
  
  ?item wdt:P31 wd:Q5 .
  ?item wdt:P106 wd:Q214917 .
  ?item wdt:P569 ?ddn .
  ?item wdt:P27 wd:Q38 .
  OPTIONAL { ?item wdt:P8248 ?cc . }

  FILTER((?ddn >= \"1900-01-01T00:00:00Z\"^^xsd:dateTime) && (?ddn<=\"1999-12-31T00:00:00Z\"^^xsd:dateTime))
    SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY ?ddn""";
