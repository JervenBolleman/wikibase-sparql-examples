  ?article schema:about ?item. # якія маюць артыкул
  ?article schema:isPartOf <https://be-tarask.wikipedia.org/>. # у *беларускай* Вікіпэдыі

  SERVICE wikibase:label { bd:serviceParam wikibase:language \"be-tarask,uk,en,ru\". }
} GROUP BY ?sexLabel
ORDER BY DESC(?count)""";
