    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org/wiki/Wikidata:WikiProject_The_Commemorative_Association_of_the_National_Debt_Redemption_Movement_(NDRM)/Queries>;
