     BIND(xsd:integer(REPLACE(?label,'\\\\D','')) AS ?number)
     FILTER(?number<100)
     SERVICE wikibase:label { bd:serviceParam wikibase:language 'en'}
     ?wikilink schema:about ?id; schema:isPartOf <https://en.wikipedia.org/> .
}
ORDER BY ?number""";
