@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-5f7dec9595a1e7a8292f3fddabc4ff5a> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """# CWGC person ID with enwiki link
SELECT DISTINCT ?s ?sLabel ?article WHERE {
  ?s wdt:P1908 ?cwgcid .
    ?article schema:about ?s .
    ?article schema:inLanguage \"en\" .
    ?article schema:isPartOf <https://en.wikipedia.org/> .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\" }
 }
 ORDER BY ?sLabel""";
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User_talk:Bamyers99>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql> .
