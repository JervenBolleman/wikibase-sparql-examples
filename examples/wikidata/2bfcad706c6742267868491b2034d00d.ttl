<https://www.wikidata.org/#query-2bfcad706c6742267868491b2034d00d> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?city ?cityLabel 
WHERE {
  ?city wdt:P31/wdt:P279* wd:Q515;
        wdt:P131 wd:Q232. 
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"de,en\". }
}
ORDER BY ?cityLabel""";
