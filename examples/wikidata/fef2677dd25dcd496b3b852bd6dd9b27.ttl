    FILTER((LANG(?lhi)) = \"hi\")
  }
  OPTIONAL {
    ?item schema:description ?den.
    FILTER((LANG(?den)) = \"en\")
  }
  FILTER(NOT EXISTS {
    ?item schema:description ?dhi.
    FILTER((LANG(?dhi)) = \"hi\")
  })""";
  schema:target <https://query.wikidata.org/sparql> .
