PREFIX bd: <http://www.bigdata.com/rdf#>
#Comunes dels Pirineus Orientals segons què les banya
SELECT ?lloc ?layer ?coord ?llocLabel ?layerLabel WHERE {
  ?lloc wdt:P131* wd:Q12709.
  ?lloc wdt:P31 wd:Q484170.
