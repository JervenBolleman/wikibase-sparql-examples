SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\".
                        ?country rdfs:label ?countryLabel. ?element rdfs:label ?elementLabel}
} GROUP BY ?countryLabel
ORDER BY DESC (?count)""";
