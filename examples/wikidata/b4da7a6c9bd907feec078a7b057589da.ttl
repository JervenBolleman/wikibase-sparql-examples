<https://www.wikidata.org/#query-b4da7a6c9bd907feec078a7b057589da> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Back_ache/ListenNotes>;
