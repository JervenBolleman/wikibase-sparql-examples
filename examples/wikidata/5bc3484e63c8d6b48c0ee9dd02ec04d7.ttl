<https://www.wikidata.org/#query-5bc3484e63c8d6b48c0ee9dd02ec04d7> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#added before 2016-10

SELECT ?name (COUNT(?otherName) AS ?count)
WHERE
{
  ?name wdt:P31 wd:Q202444;
        wdt:P460 ?otherName.
}
GROUP BY ?name
ORDER BY DESC(?count)
LIMIT 10""";
