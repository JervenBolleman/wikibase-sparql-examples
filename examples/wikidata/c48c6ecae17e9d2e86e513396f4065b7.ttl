<https://www.wikidata.org/#query-c48c6ecae17e9d2e86e513396f4065b7> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Jobu0101/Tatort>;
