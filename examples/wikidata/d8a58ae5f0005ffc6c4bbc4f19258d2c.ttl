<https://www.wikidata.org#query-d8a58ae5f0005ffc6c4bbc4f19258d2c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org/wiki/User:MartinPoulter/queries/booker>;
