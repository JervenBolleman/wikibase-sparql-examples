#defaultView:Map
SELECT ?item ?itemLabel ?property ?propertyLabel ?coord
WHERE 
{ 
  VALUES ?item { wd:Q3237 }
  ?item p:P625 ?stat1 .
#  ?property wikibase:claim ?pred1 .
  ?stat1 psv:P625 ?stat2 .
