  sh:select """#----------------------------------------------------------
# many familynames
#----------------------------------------------------------
SELECT ?item ?itemLabel 
WHERE 
{
  SERVICE bd:slice {
    ?item wdt:P31 wd:Q101352  .
    bd:serviceParam bd:slice.offset 0 .
    bd:serviceParam bd:slice.limit 2000 .
  }      
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],de\". } 
}""";
