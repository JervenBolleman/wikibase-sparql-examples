<https://www.wikidata.org/#query-706eb5904304ef9e33a5c6e29dff7d28> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Bouzinac/Fr%C3%A9quentations>;
