<https://www.wikidata.org#query-e8910c7fb9ef85d2b42f170b47d53064> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org/wiki/User:Bouzinac/Recherches_d%27a%C3%A9roports>;
