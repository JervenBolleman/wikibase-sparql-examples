  sh:select """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
SELECT DISTINCT ?item
WHERE {
  ?item wdt:P691 ?nkc1 .
  ?item wdt:P691 ?nkc2 .
  FILTER(?nkc1 != ?nkc2)
  ?item wdt:P496 ?orcid .
}""";
