<https://www.wikidata.org#query-9d667f07d83bde91c38f56ba6d7d79e8> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org/wiki/User:Jobu0101/Tatort>;
