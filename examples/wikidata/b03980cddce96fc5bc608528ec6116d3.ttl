<https://www.wikidata.org/#query-b03980cddce96fc5bc608528ec6116d3> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?aggregate ?aggregateLabel (count(?w) as ?number)
WHERE 
{
  ?w wdt:P463 wd:Q105757481 .
  ?w wdt:P749 ?aggregate .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"de,en\" . }
} GROUP BY ?aggregate ?aggregateLabel
ORDER BY DESC(?number)""";
  rdfs:comment "TODO"@en;
