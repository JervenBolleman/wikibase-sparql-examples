<https://www.wikidata.org/#query-ade6d836dcd680aa785354aa1243f88c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:JotaCartas/Queries>;
