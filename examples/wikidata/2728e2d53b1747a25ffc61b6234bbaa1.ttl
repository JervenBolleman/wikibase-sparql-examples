<https://www.wikidata.org/#query-2728e2d53b1747a25ffc61b6234bbaa1> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT * WHERE {
  ?q wdt:P481 ?id ; rdfs:label ?label .
  FILTER (lang(?label) = \"fr\"). 
  FILTER regex (?label, \"(V|v)ierge\").
  MINUS { ?q wdt:P180 [] }
}""";
