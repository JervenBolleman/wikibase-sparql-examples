<https://www.wikidata.org/#query-0687056ba5f5e3cfacac6759d2bc8f73> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """# Every MSC station still in use
#defaultView:Map
SELECT ?item ?itemLabel ?coord 
WHERE {
  ?item (wdt:P31/wdt:P279*) wd:Q190107.
  ?item wdt:P127 wd:Q349450.

  OPTIONAL { ?item wdt:P3999 ?closureDate }
  FILTER(# Closure date is not set
    !BOUND(?closureDate)    
  )
   
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
  OPTIONAL { ?item wdt:P625 ?coord. }
}""";
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:WikiProject_Weather_observations/en/SPARQL>;
