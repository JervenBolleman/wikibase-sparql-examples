<https://www.wikidata.org/#query-1ddd3af062bb1fb2f60a6190b101da96> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#Stations du SMC par altitude, par groupe de 500 m.
#defaultView:Map
SELECT ?item ?itemLabel ?coord ?altitude ?layer WHERE {
  ?item (wdt:P31/wdt:P279*) wd:Q190107 ; wdt:P127 wd:Q349450 ; wdt:P2044 ?altitude .
  BIND (FLOOR(?altitude/500) AS ?layer )
  ?item wdt:P625 ?coord.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:WikiProject_Weather_observations/SPARQL>;
