  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  sh:select """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
SELECT ?type (COUNT(DISTINCT ?item) AS ?count) WHERE {
  ?item wdt:P8034 ?id .
  ?item wdt:P106 ?occ .
  BIND(?occ AS ?type)
}
GROUP BY ?type
ORDER BY DESC(?count)""";
