 }
   ?cast rdfs:label ?castLabel . filter(lang(?castLabel)=\"es\")

  } } as %i
where
{
   INCLUDE %i
   SERVICE wikibase:mwapi
 {
   bd:serviceParam wikibase:endpoint \"es.wikipedia.org\" .
