   bd:serviceParam mwapi:generator \"categorymembers\" .
   bd:serviceParam mwapi:gcmtitle \"Categoría:Iglesias de Extremadura por siglo\" .
   bd:serviceParam mwapi:gcmlimit \"max\" .
   bd:serviceParam mwapi:gcmnamespace \"14\" .
