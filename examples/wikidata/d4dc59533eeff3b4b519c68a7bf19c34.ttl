<https://www.wikidata.org/#query-d4dc59533eeff3b4b519c68a7bf19c34> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?wikidata_id ?wikidata_idLabel (?wikidata_idLabel as ?name) ?dc_id ?party_id ?twitter_username
WHERE 
{
  ?wikidata_id p:P3602 ?ps .
  ?ps ps:P3602 wd:Q96370964 .
  OPTIONAL { ?wikidata_id wdt:P6465 ?dc_id }
  OPTIONAL { ?wikidata_id wdt:P21 ?gender }
  OPTIONAL { ?wikidata_id wdt:P2002 ?twitter_username }
  OPTIONAL { ?ps pq:P102 ?party_id . }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
