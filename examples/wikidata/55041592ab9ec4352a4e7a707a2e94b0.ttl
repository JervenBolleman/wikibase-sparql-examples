<https://www.wikidata.org#query-55041592ab9ec4352a4e7a707a2e94b0> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org/wiki/User:Oravrattas/prompts/Argentina/Chamber_of_Deputies>;
