<https://www.wikidata.org/#query-2ad43be7c8334fdb782420810fe76015> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """# Categories de guanyadors per premis
SELECT DISTINCT ?premi ?premiLabel ?cat ?categoria
WHERE {
  ?premi wdt:P31/wdt:P279* wd:Q21142755.
  ?premi wdt:P2517 ?cat.
    ?categoria schema:about ?cat.
    ?categoria schema:isPartOf <https://ca.wikipedia.org/>.
SERVICE wikibase:label {
bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],ca,en,es\" . } 
}""";
