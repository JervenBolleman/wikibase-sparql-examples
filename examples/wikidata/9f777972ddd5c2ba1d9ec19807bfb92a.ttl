<https://www.wikidata.org/#query-9f777972ddd5c2ba1d9ec19807bfb92a> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#title:Number of researchers with fundamental data and affiliation to institutions in Switzerland or Liechtenstein by gender
SELECT ?gender ?genderLabel (COUNT(DISTINCT ?person) AS ?number)
WHERE {
  ?person wdt:P21 ?gender .
  ?person wdt:P735 [ ] .
  ?person wdt:P734 [ ] .
  ?person wdt:P108 [ wdt:P17 ?country ] . VALUES ?country { wd:Q39 wd:Q347 } .
  ?person wikibase:identifiers ?n . FILTER(?n > 0)
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}
GROUP BY ?gender ?genderLabel ?number""";
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:WikiCite/Researchers_in_Switzerland/Queries>;
