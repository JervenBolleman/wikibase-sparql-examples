<https://www.wikidata.org/#query-2d9d58fdae4ee47d5e234b3f7c9d792d> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#defaultView:ImageGrid{\"hide\": [\"?image\"]}
SELECT ?image ?personDescription ?personLabel ?prmlink
WHERE {
?person p:P485 [ps:P485 wd:Q1456119; pq:P973 ?prmlink].
?person wdt:P18 ?image
SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\" }
}""";
