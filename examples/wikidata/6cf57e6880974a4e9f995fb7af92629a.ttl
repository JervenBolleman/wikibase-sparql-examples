<https://www.wikidata.org/#query-6cf57e6880974a4e9f995fb7af92629a> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Mfchris84/Tagung_Digitale_Bibliothek_2019>;
