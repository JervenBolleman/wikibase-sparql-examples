<https://www.wikidata.org/#query-0008c56676c71f37e9a9d97c637e9cf6> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#Trouve tous les éléments connus de Wikidata qui sont des \"archives départementales\"
SELECT ?item ?itemLabel 
WHERE 
{
  ?item wdt:P31 wd:Q2860456.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
