  BIND(if(sameTerm(?state_, wd:Q1361864), \"tax. nov.\", if(sameTerm(?state_, wd:Q14594740), \"comb. nov.\", if(sameTerm(?state_, wd:Q749462), \"nom. nov.\", \"\"))) as ?state) 
