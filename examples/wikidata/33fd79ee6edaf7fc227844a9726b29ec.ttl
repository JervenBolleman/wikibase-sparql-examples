<https://www.wikidata.org/#query-33fd79ee6edaf7fc227844a9726b29ec> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?cookbook ?cookbookLabel ?worldcatURI
WHERE
{
  ?cookbook wdt:P31|wdt:P136 wd:Q605076.
  OPTIONAL {
    ?cookbook wdt:P243 ?ocn.
    BIND (URI(CONCAT(\"http://www.worldcat.org/oclc/\",?ocn)) AS ?worldcatURI)
  }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\" }
}""";
