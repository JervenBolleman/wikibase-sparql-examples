    {?subj wdt:P1120 ?dead.} UNION {?subj wdt:P793 ?discat}.
    OPTIONAL {?subj wdt:P1120 ?dead}.
    ?subj    wdt:P625 ?loc;
            p:P625/psv:P625 ?coord.
    ?coord wikibase:geoLatitude ?lat .
    ?coord wikibase:geoLongitude ?long .
    FILTER (?lat > 55.6) FILTER (?lat < 55.9)
