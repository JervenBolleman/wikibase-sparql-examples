  OPTIONAL { ?format wdt:P31 ?instanceof } .
  OPTIONAL { ?format wdt:P279 ?subclassof }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\" }
} 
ORDER BY ?ffw""";
