<https://www.wikidata.org/#query-588598ad32881ac5f2c229300a41a202> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel (YEAR(?dod) as ?year_of_death)
WHERE
{
  ?item wdt:P27 wd:Q801;
        wdt:P570 ?dod.
  FILTER(YEAR(?dod) = 1970)
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\" }
}""";
  rdfs:comment "TODO"@en;
