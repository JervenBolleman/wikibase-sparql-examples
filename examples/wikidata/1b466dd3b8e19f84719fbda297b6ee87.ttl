<https://www.wikidata.org/#query-1b466dd3b8e19f84719fbda297b6ee87> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Sintakso/SPARQL>;
