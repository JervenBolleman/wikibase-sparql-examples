<https://www.wikidata.org#query-bd1dc6cc1e19dadc670215e310b57c3c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org/wiki/User:Hjart/SPARQL_queries>;
