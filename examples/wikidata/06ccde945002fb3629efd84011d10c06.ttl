<https://www.wikidata.org/#query-06ccde945002fb3629efd84011d10c06> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Una_tantum>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
# Doppioni Tourer.it a Bologna > Unique value constraint report for P4133: report listing each item
SELECT DISTINCT ?item1 ?item1Label ?item2 ?item2Label ?value
{
	?item1 wdt:P4133 ?value .
	?item2 wdt:P4133 ?value .
    ?item1 (wdt:P131)* wd:Q1891
	FILTER( ?item1 != ?item2 && STR( ?item1 ) < STR( ?item2 ) ) .
	SERVICE wikibase:label { bd:serviceParam wikibase:language \"en,it,fr\" } .
}
LIMIT 100""" .
