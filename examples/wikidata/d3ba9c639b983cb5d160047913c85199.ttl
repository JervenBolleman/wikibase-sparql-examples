<https://www.wikidata.org/#query-d3ba9c639b983cb5d160047913c85199> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?item ?itemLabel (SAMPLE(?img) AS ?sampleimg)
WHERE {
  ?item wdt:P31 wd:Q5.
  ?item wdt:P106 wd:Q169470.
  ?item wdt:P1801 ?img .
  
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
} GROUP BY ?item ?itemLabel""";
