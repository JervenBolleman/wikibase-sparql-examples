  FILTER(xsd:integer(STRAFTER(STR(?item1), 'Q')) < xsd:integer(STRAFTER(STR(?item2), 'Q'))) .
  ?article1 schema:about ?item1;
    schema:isPartOf <https://ko.wikipedia.org/>.
  ?article2 schema:about ?item2;
