  sh:select """#Instance of (instance of pharmaceutical drug)
SELECT ?item ?itemLabel ?dummy ?dummyLabel
WHERE {
  
  ?item wdt:P31 ?dummy .
  ?dummy wdt:P31 wd:Q12140 .
  
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\" }
}""";
