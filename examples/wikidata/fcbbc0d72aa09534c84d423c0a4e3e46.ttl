<https://www.wikidata.org/#query-fcbbc0d72aa09534c84d423c0a4e3e46> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Gruppo_Wikidata_per_Musei,_Archivi_e_Biblioteche/Bibliografia_Carlo_Revelli>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
# Item di pubblicazioni di Carlo Revelli
SELECT ?titolo ?titoloLabel ?tipologiaLabel ?pagine ?paginetot ?annopubblicazione ?venue ?venueLabel
WHERE { ?titolo wdt:P50 wd:Q22093383 .
       
       OPTIONAL { ?titolo wdt:P304 ?pagine .}
       OPTIONAL { ?titolo wdt:P1104 ?paginetot .} 
       OPTIONAL { ?titolo wdt:P577 ?annopubblicazione .} 
       OPTIONAL { ?titolo wdt:P1433 ?venue .} 
       OPTIONAL { ?titolo wdt:P31 ?tipologia .}
       
       SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],it\". }
       
       }

ORDER BY ?titoloLabel""";
