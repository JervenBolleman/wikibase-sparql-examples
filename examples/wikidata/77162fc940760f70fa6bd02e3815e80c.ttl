<https://www.wikidata.org/#query-77162fc940760f70fa6bd02e3815e80c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """# Numero di item che sono istanza di 'opera scritta' e hanno erroneamente anche numero di ISBN a 13 o a 10 cifre 
SELECT ?item ?itemLabel ?ISBN_13 ?ISBN_10 WHERE {
  
  ?item wdt:P31 wd:Q47461344 .
  { ?item wdt:P212 ?ISBN_13  } UNION {?item wdt:P957 ?ISBN_10 }
    SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE]it,en\". }
}""";
