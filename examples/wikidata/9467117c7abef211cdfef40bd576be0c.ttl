  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX bd: <http://www.bigdata.com/rdf#>
# Authorship gender distribution (not distinct)

PREFIX target: <http://www.wikidata.org/entity/Q2128181>

SELECT ?count ?gender ?genderLabel 

WHERE {
  {
  SELECT (COUNT(?author) AS ?count) ?gender WHERE {
    ?work wdt:P1433 target: .
    ?work wdt:P50 ?author .
    ?author wdt:P21 ?gender .
  } 
  GROUP BY ?gender
}
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\" . } 
} 
 ORDER BY DESC(?count)""";
  <http://example.org/bigdata_select> """# Authorship gender distribution (not distinct)

PREFIX target: <http://www.wikidata.org/entity/Q2128181>

SELECT ?count ?gender ?genderLabel 
WITH {
  SELECT (COUNT(?author) AS ?count) ?gender WHERE {
    ?work wdt:P1433 target: .
    ?work wdt:P50 ?author .
    ?author wdt:P21 ?gender .
  } 
  GROUP BY ?gender
} AS %result
WHERE {
  INCLUDE %result
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\" . } 
} 
 ORDER BY DESC(?count)""",
