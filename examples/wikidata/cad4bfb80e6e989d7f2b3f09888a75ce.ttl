  }
  BIND(URI(CONCAT(STR(?class), \"#nodeShape\")) AS ?nodeShape)
  BIND(URI(CONCAT(STR(?wdt), \"#wdtShape\")) AS ?wdtShape)
  ?prop wikibase:directClaim ?wdt ;
        p:P2302 [
          ps:P2302 wd:Q21503250 ;
          pq:P2308 ?domain
        ] ;
        p:P2302 [
