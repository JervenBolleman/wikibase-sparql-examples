<https://www.wikidata.org/#query-0988b50eb8fd42a787fde4d7a11e11aa> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?itemLabel (LCASE(?twitter_mc) AS ?twitter) ?constituencyLabel
WHERE 
{
  ?item p:P39 ?statement .
  ?statement ps:P39/wdt:P279* wd:Q16707842 ; pq:P580 ?start .
  OPTIONAL { ?statement pq:P582  ?end }
  OPTIONAL { ?statement pq:P768  ?constituency }
  ?item wdt:P2002 ?twitter_mc .
  FILTER(!BOUND(?end) || ?end > NOW())
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY ?twitter""";
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Jacksonj04/UK_MP_Twitter_Accounts>;
