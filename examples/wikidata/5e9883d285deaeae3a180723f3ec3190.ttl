<https://www.wikidata.org/#query-5e9883d285deaeae3a180723f3ec3190> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """# Mappa dei luoghi di nascita dei botanici italiani (un po’ lenta …)
#defaultView:Map
SELECT ?item ?itemLabel ?coord
WHERE {
  ?item wdt:P31 wd:Q5 . # item di tipo 'umano'
  ?item wdt:P106 wd:Q2374149 . # occupazione \"botanico\" 
  ?item wdt:P27 wd:Q38 . # di cittadinanza \"Italia\"
  ?item wdt:P19 ?luogodinasc . # luogo di nascita
  ?luogodinasc wdt:P625 ?coord . #coordinate del luogo di nascita
  SERVICE wikibase:label {bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
