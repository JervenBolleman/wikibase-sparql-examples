  rdfs:comment "ðŸª£ Taxa with a Global Invasive Species Database ID (P5626) or an Invasive Species Compendium Datasheet ID (P5698), ranked by number of Wikimedia sitelinks"@en;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
#defaultView:ImageGrid
SELECT DISTINCT ?item ?taxonname (SAMPLE(?image_) AS ?image) ?linkcount WHERE {

  {?item wdt:P5698 [].} UNION {?item wdt:P5626 [].}
#   ?item wdt:P6104 wd:Q56241615 . 
  ?item wdt:P18 ?image_ ;
        wdt:P225 ?taxonname.
  ?work wdt:P921 ?item .
  ?item wikibase:sitelinks ?linkcount .
}
GROUP BY ?item ?taxonname ?image ?linkcount
ORDER BY DESC(?linkcount)""";
