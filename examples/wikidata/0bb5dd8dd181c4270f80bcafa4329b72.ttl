<https://www.wikidata.org/#query-0bb5dd8dd181c4270f80bcafa4329b72> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#title:Lexemes in English and the names of their lexical categories
SELECT ?lexeme ?category ?categoryLabel {
    ?lexeme dct:language wd:Q1860 .
    ?lexeme wikibase:lexicalCategory ?category .
    SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
