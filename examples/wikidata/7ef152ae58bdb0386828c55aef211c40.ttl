<https://www.wikidata.org#query-7ef152ae58bdb0386828c55aef211c40> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org/wiki/User:Mat%C4%9Bj_Such%C3%A1nek/Cleanup>;
