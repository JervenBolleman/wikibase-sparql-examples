<https://www.wikidata.org/#query-b2a0d542dea53a6e09cd7d54658ecac2> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """# villages that should have P31 = Q3558970, but have Q532
SELECT ?item ?itemLabel ?adminLabel
WHERE
{
	?item wdt:P31 wd:Q532 .
  	?item wdt:P17 wd:Q36 .
  	?item wdt:P131 ?admin
	SERVICE wikibase:label { bd:serviceParam wikibase:language \"pl\" }
}""";
