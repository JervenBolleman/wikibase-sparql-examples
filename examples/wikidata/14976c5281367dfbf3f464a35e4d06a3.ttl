    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Trilotat/SPARQL>, <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2019/01>;
