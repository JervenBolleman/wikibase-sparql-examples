<https://www.wikidata.org#query-e40b8fac93ffd8bdaae6204984416e7b> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org/wiki/User:Deryck_Chan/SPARQL>;
