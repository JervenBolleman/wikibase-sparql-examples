  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  sh:select """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
SELECT ?o (count(?o) as ?count) WHERE {
  ?id wdt:P31 ?o.
}
group by ?o 
having (?count > 500000)
order by desc(?count)""";
