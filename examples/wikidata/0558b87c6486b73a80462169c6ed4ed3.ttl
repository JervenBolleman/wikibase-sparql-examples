  ?People_s_Republic_of_China wdt:P27 wd:Q148.
  ?People_s_Republic_of_China wdt:P1399 ?criminal.
  ?criminal rdfs:label ?label.
  FILTER(LANG(?label) = \"zh-cn\")
}
