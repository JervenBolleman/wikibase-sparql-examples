  BIND(xsd:integer(?ref) AS ?refnumber)
  BIND(xsd:integer(REPLACE(?page, \"-[0-9]*\", \"\")) AS ?startpage)
  BIND(xsd:integer(REPLACE(?page, \"[0-9]*-\", \"\")) AS ?endpage) 
}
ORDER BY ASC(?refnumber)""";
