<https://www.wikidata.org/#query-8bd50856a37a8690561e6db8e630977c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?lexemeId ?lemma ?dicoentry ?refnumber ?startpage ?endpage WHERE {
  ?lexemeId dct:language wd:Q150 ; wikibase:lemma ?lemma ; p:P1343 ?dico.
  ?dico ps:P1343 wd:Q115664643 ; pq:P304 ?page ; pq:P958 ?ref ; pq:P1932 ?dicoentry.
  BIND(xsd:integer(?ref) AS ?refnumber)
  BIND(xsd:integer(REPLACE(?page, \"-[0-9]*\", \"\")) AS ?startpage)
  BIND(xsd:integer(REPLACE(?page, \"[0-9]*-\", \"\")) AS ?endpage) 
}
ORDER BY ASC(?refnumber)""";
