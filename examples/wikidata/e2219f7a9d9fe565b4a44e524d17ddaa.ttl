<https://www.wikidata.org/#query-e2219f7a9d9fe565b4a44e524d17ddaa> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?gender ?genderLabel (COUNT(*) AS ?count)  WHERE {
  VALUES ?target {
    wd:Q385468
    wd:Q17524931
    wd:Q18093037
  }
  ?item wdt:P31 wd:Q5;
    wdt:P21 ?gender;
    p:P735 ?statement.
  ?statement ps:P735  ?target.
  OPTIONAL { ?statement pq:P1545 ?rank. }
  BIND(IF(BOUND(?rank),?rank, 0) AS ?rank2)
  FILTER(xsd:integer(?rank2) < xsd:integer(2))
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"fr,en\". }
}
GROUP BY  ?gender ?genderLabel
ORDER BY DESC (?count)""";
