SELECT ?item ?itemLabel ?typeLabel WHERE {
  VALUES ?bad_office {
    wd:Q30185 # mayor
    }
  ?item wdt:P2388|wdt:P1313 ?bad_office .
  ?item wdt:P17 wd:Q928 .
