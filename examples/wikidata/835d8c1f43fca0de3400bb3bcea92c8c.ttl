  sh:select """#check for RTC ratings with the same rating ID
SELECT ?item1 ?item2
WHERE {
  ?item1 p:P3834 [ pq:P2676 ?r ] .
  ?item2 p:P3834 [ pq:P2676 ?r ] .
  FILTER ( ?item1 != ?item2 )
}""";
