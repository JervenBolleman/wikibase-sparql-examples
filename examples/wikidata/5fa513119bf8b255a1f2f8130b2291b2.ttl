<https://www.wikidata.org/#query-5fa513119bf8b255a1f2f8130b2291b2> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Herzi_Pinki/Sparql/Coordinate_Me>;
