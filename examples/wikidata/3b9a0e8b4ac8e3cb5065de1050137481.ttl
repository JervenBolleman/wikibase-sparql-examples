<https://www.wikidata.org/#query-3b9a0e8b4ac8e3cb5065de1050137481> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?item ?itemLabel ?coords WHERE {
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en-GB,en,mul\". }
  {
    SELECT DISTINCT ?item ?coords WHERE {
      ?item p:P31 ?statement0.
      ?statement0 (ps:P31/(wdt:P279*)) wd:Q57821.
      {?item wdt:P17  wd:Q45} 
      UNION 
      {?item wdt:P17  wd:Q29}
      ?item wdt:P625 ?coords.
    }
  }
}""";
