<https://www.wikidata.org#query-a990ae4755667fcaf57c735f7ab9192a> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org/wiki/User:YULdigitalpreservation/SPARQL>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  rdfs:comment "Smith College1. Return an image grid of all people whose archival papers are held at Smith College:"@en;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#defaultView:ImageGrid
SELECT  ?personLabel  ?image

WHERE {
  
  ?person wdt:P485 wd:Q49204 .
  ?person wdt:P18 ?image
  
  SERVICE wikibase:label {
		bd:serviceParam wikibase:language \"en\" .
    
}
  }""";
