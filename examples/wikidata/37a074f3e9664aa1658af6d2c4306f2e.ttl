@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-37a074f3e9664aa1658af6d2c4306f2e> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:WikiProject_Sculpture>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX schema: <http://schema.org/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#title:Wikis with pages about individual sculptures
SELECT ?wiki ?wikinameLabel (COUNT(?site) AS ?count) WHERE {
INCLUDE %toplist.
?site schema:about ?item ; schema:isPartOf ?wiki .
  OPTIONAL { ?wikiname wdt:P856 ?wiki }
 SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
} GROUP BY ?wiki ?wikinameLabel ORDER BY DESC(?count)""";
  <http://example.org/bigdata_select> """#title:Wikis with pages about individual sculptures
SELECT ?wiki ?wikinameLabel (COUNT(?site) AS ?count) WITH {SELECT DISTINCT ?item WHERE {
?item wdt:P31/wdt:P279? wd:Q860861 } } AS %toplist WHERE {
INCLUDE %toplist.
?site schema:about ?item ; schema:isPartOf ?wiki .
  OPTIONAL { ?wikiname wdt:P856 ?wiki }
 SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
} GROUP BY ?wiki ?wikinameLabel ORDER BY DESC(?count)""" .
