{
    ?item wdt:P39 wd:Q18510612 .  
    ?item wdt:P569 ?dob .
    ?item wdt:P570 ?dod .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}
GROUP BY ?item ?itemDescription""";
