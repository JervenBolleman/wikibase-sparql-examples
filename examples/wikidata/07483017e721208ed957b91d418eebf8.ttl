<https://www.wikidata.org/#query-07483017e721208ed957b91d418eebf8> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT DISTINCT ?item ?itemLabel ?itemDescription ?orcid ?organization ?organizationLabel ?organizationDescription ?p WHERE {
  ?item wdt:P496 ?orcid.
  ?item (p:P69|p:P108) [ (ps:P69|ps:P108) ?organization ] .
  ?organization (wdt:P361|wdt:P749)* wd:Q13371 .
  ?item ?p ?organization .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\" }
}""";
