  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#defaultView:Table
# Publications citing papers from this page
SELECT (min(?dates) as ?date) ?work ?workLabel WHERE {
  VALUES ?work1 { wd:Q29192850 wd:Q24805506 wd:Q25257880 wd:Q29028332 wd:Q28382972 wd:Q27003978 wd:Q29192850 wd:Q24696016 wd:Q24705267 wd:Q28080273 wd:Q28950514 wd:Q28956355 } # place paper collection here, e.g. \"wd:Q28976290 wd:Q28976313\"
  ?work wdt:P2860 ?work1 .
  optional { ?work wdt:P577 ?dates . }
  SERVICE wikibase:label {
    bd:serviceParam wikibase:language \"en,fr,de,ru,es,zh,jp\".
  }
}
group by ?work ?workLabel
order by desc(?date)
LIMIT 1000""";
