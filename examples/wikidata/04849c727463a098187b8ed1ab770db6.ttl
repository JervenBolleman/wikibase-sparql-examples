<https://www.wikidata.org/#query-04849c727463a098187b8ed1ab770db6> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#title: Most common P31 for entities that develop games
SELECT ?type ?typeLabel (COUNT(?game) as ?games) WHERE {
  ?game wdt:P31 wd:Q7889.
  ?game wdt:P178 ?developer.
  ?developer wdt:P31 ?type.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
} GROUP BY ?type ?typeLabel
ORDER BY DESC(?games)""";
