  ?item wdt:P31 ?inst.
  ?item wdt:P131+ wd:Q1492.
  ?item wdt:P625 ?coord.
  OPTIONAL {
    ?item wdt:P131 ?layer.
    ?layer wdt:P31 wd:Q75135432.
  }
  SERVICE wikibase:label {
    bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],ca,en,es,fr,de\" . 
  }
}
#defaultView:Map""";
