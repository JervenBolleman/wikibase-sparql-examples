PREFIX target: <http://www.wikidata.org/entity/Q365068>

SELECT 
  ?artwork ?artworkLabel
  ?image

WHERE {
  ?artwork wdt:P170 target: ;
           wdt:P18 ?image
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
