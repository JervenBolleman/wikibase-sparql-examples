@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-e44a8f97bd228336a0a7c3372565bd91> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "TODO"@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:YanikB/SPARQL>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX pq: <http://www.wikidata.org/prop/qualifier/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#Municipalités existantes au Québec créées en 1855
SELECT DISTINCT ?item ?itemLabel ?statutLabel ?mrcLabel ?regionLabel ?dateCreation
WHERE 
{
    ?item(wdt:P31*/wdt:P279*) wd:Q3327873 .
    ?item wdt:P31 ?statut .
    ?item wdt:P131 ?mrc .
    ?mrc wdt:P131 ?region .
    ?item wdt:P571 ?dateCreation .
    FILTER (?dateCreation >= \"1855-01-01T00:00:00Z\"^^xsd:dateTime ) .
    FILTER (?dateCreation < \"1856-01-01T00:00:00Z\"^^xsd:dateTime ) .
    FILTER NOT EXISTS { ?item wdt:P576 ?dissolu}
    FILTER NOT EXISTS { ?item wdt:P582 ?fin }
    FILTER NOT EXISTS { ?item p:P31 [ pq:P582 ?pqfin ] }
    SERVICE wikibase:label {bd:serviceParam wikibase:language \"fr\" }
} 
ORDER BY ?itemLabel
LIMIT 1000""" .
