<https://www.wikidata.org/#query-e33dc5a4d312c739c75f0b69164a3929> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?a ?aLabel ?p ?pLabel ?w ?wLabel
WHERE { 
  {
    SELECT ?a ?w ?p
    WHERE {  
      ?p wdt:P31 wd:Q98270496 . 
      ?p wdt:P1416 ?w .
      OPTIONAL {
      ?w wdt:P131+ ?a .
      ?a  wdt:P31 wd:Q1221156 .
      }
    }
  }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"de\" . }
} ORDER BY ASC(?aLabel) ASC(?pLanbel) ASC(?wLabel)""";
