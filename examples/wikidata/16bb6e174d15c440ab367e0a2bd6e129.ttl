<https://www.wikidata.org/#query-16bb6e174d15c440ab367e0a2bd6e129> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Aeroid/SPARQL_Notes>;
