 FILTER NOT EXISTS {?con wdt:P576 ?end} # Not abolished
 OPTIONAL{ ?enwp schema:about ?con. ?enwp schema:isPartOf <https://en.wikipedia.org/>}
SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\"}
} ORDER BY ?conLabel""";
