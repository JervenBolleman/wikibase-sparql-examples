@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-8d6cdce1a48664bcf50a79af57970672> a sh:SPARQLExecutable,
    sh:SPARQLConstructExecutable;
  sh:construct """PREFIX wdt:  <http://www.wikidata.org/prop/direct/>
PREFIX wikibase:  <http://wikiba.se/ontology#>

CONSTRUCT {
  ?ocnURI ?schemaP ?viafURI.
} WHERE {
  ?s wdt:P243 ?ocn ;
#     wdt:P31 ?sType ;
     ?p ?o .
  ?wd  wikibase:directClaim ?p.
  ?wd wdt:P1628 ?schemaP.
  ?o wdt:P214 ?viafID ;
#     wdt:P31 ?oType
     .
  BIND(URI(CONCAT('http://www.worldcat.org/oclc/', ?ocn)) AS ?ocnURI)
  BIND(URI(CONCAT('http://viaf.org/viaf/', ?viafID)) AS ?viafURI)
  FILTER(CONTAINS(STR(?schemaP), \"schema.org\"))
#  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\" }
}
LIMIT 100""";
  rdfs:comment "TODO"@en;
