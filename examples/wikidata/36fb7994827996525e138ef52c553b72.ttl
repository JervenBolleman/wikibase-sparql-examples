SELECT ?item ?page_title ?article (GROUP_CONCAT(DISTINCT ?instanz; SEPARATOR = \", \") AS ?ist_ein) (GROUP_CONCAT(DISTINCT ?standort; SEPARATOR = \", \") AS ?liegt_in) ?dawiki ?location WHERE {
