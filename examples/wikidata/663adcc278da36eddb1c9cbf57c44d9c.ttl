  BIND(xsd:integer(substr(str(?lexeme), 33)) as ?lno)
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],ml\". }
}
ORDER BY ?lno
LIMIT 100""";
