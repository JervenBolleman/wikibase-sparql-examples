     BIND(day(?dob) as ?giorno_nascita).
     BIND(month(?dob) as ?mese_nascita).
     BIND(year(?dob) as ?anno_nascita).
     
     OPTIONAL {?item wdt:P18 ?image.}
     MINUS {
