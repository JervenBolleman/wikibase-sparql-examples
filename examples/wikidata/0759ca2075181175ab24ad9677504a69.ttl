    ?wp wdt:P407 ?l .
    ?l wdt:P218 ?iso6391 .
    ?l wdt:P219 ?iso6392 .
    ?l wdt:P220 ?iso6393 .
    
    SERVICE wikibase:label {
        bd:serviceParam wikibase:language \"en\" .
    }
 } ORDER BY ?wpLabel""";
