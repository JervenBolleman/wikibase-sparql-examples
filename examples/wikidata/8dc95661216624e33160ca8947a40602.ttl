<https://www.wikidata.org/#query-8dc95661216624e33160ca8947a40602> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """select ?short_author (count(?paper) as ?count) where {
  
  ?paper wdt:P1325 ?external .
  filter (strstarts(str(?external), 'http://neurovault.org/collections/'))
  ?paper wdt:P2093 ?short_author .
  }
group by ?short_author
order by desc(?count)""";
