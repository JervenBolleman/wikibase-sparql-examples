  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  schema:target <https://query.wikidata.org/sparql>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT DISTINCT ?item ?itemLabel (GROUP_CONCAT(DISTINCT ?P31Label_;separator=\"; \") as ?P31Label)
WHERE 
{
  { ?item wdt:P31/wdt:P279*  wd:Q201658 . } UNION
  { ?item wdt:P31/wdt:P279* wd:Q659563 . } UNION
  { ?item wdt:P31/wdt:P279* wd:Q223393 . } UNION
  { ?item wdt:P31/wdt:P279* wd:Q188451 . } UNION
  { ?item wdt:P31/wdt:P279* wd:Q4178140 . } UNION
  { ?item wdt:P31/wdt:P279* wd:Q104822033. }
  
  ?item wdt:P31 ?P31.  hint:Prior hint:runLast true.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". 
                         ?item rdfs:label ?itemLabel . 
                         ?P31 rdfs:label ?P31Label_ . } 
} GROUP BY ?item ?itemLabel""",
