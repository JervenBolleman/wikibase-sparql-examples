<https://www.wikidata.org/#query-0b3f43f2e1bfd1e40b9668f414cabb99> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?p ?pLabel (GROUP_CONCAT(?spouse; separator=\", \") AS ?spouses) (GROUP_CONCAT(?spouseLabel; separator=\", \") AS ?spouseLabels) WHERE {
  BIND(wd:Q30 AS ?country)
  ?country (p:P6/ps:P6) ?p.
  ?p wdt:P26 ?spouse.
  SERVICE wikibase:label {
    bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\".
    ?p rdfs:label ?pLabel.
    ?spouse rdfs:label ?spouseLabel.
  }
}
GROUP BY ?p ?pLabel""";
