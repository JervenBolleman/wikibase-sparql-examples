start = @<church_building>

<church_building> EXTRA wdt:P31 {
  wdt:P31 @<building> ; #type of building
  wdt:P17 @<country> * ; #country
  wdt:P276 [ <http://www.wikidata.org/entity>~ ] ? ; #location
  wdt:P625 geo:wktLiteral ? ; #geographic coordinate
