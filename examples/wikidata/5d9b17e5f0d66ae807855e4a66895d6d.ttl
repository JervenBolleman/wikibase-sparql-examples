<building> EXTRA wdt:P31 {
  wdt:P31 [wd:Q16970 wd:Q34627 wd:Q44539 wd:Q44613 wd:Q32815] + ;
}

<country> EXTRA wdt:P31 {
  wdt:P31 [wd:Q6256 wd:Q3024240 wd:Q3624078] + ;
