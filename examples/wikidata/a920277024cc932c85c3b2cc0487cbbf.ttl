  ?district wdt:P440 ?dkey .
  ?district wdt:P36 ?capital .
  ?capital wdt:P439 ?ckey
  SERVICE wikibase:label{bd:serviceParam wikibase:language \"de\"}
}
ORDER BY ?dkey""";
