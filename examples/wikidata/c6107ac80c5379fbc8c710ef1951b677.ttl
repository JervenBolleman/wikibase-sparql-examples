  sh:select """PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
PREFIX pq: <http://www.wikidata.org/prop/qualifier/>
PREFIX p: <http://www.wikidata.org/prop/>
SELECT DISTINCT ?identifier__wikidata ?name ?source (REPLACE(STR(?start),\"T00:00:00Z\",\"\") AS ?start_date) (REPLACE(STR(?end),\"T00:00:00Z\",\"\") AS ?end_date) WHERE {
  ?identifier__wikidata p:P39 ?statement.
  ?statement ps:P39 wd:Q16744266; pq:P2937 wd:Q18109299 .
  OPTIONAL { ?statement pq:P580 ?start }
  OPTIONAL { ?statement pq:P582 ?end }
  OPTIONAL {
    ?identifier__wikidata p:P973 ?describedAtPA .
    ?describedAtPA ps:P973 ?source .
    OPTIONAL {
      ?describedAtPA pq:P1810 ?name .
    }
    FILTER(CONTAINS(LCASE(STR(?source)), \"pa.org.za/\"))
  }
}""";
