<https://www.wikidata.org/#query-b5d42cdb2c95d70b415430149de10c76> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """# Distinct author gender distribution

PREFIX target: <http://www.wikidata.org/entity/Q2128181>

SELECT ?count ?gender ?genderLabel 
WITH {
  SELECT (COUNT(DISTINCT ?author) AS ?count) ?gender WHERE {
    ?work wdt:P1433 target: .
    ?work wdt:P50 ?author .
    ?author wdt:P21 ?gender .
  } 
  GROUP BY ?gender
} AS %result
WHERE {
  INCLUDE %result
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\" . } 
} 
 ORDER BY DESC(?count)""";
