OPTIONAL {?obj wdt:P31 ?type FILTER(?type != wd:Q18593264)}
OPTIONAL {?obj wdt:P18 ?image}
OPTIONAL {?obj wdt:P973 ?kc_url}
BIND(if(BOUND(?kc_url), ?kc_url, ?obj) AS ?url)
