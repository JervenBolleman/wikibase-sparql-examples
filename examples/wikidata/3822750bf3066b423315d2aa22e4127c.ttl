<https://www.wikidata.org/#query-3822750bf3066b423315d2aa22e4127c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT distinct ?item ?itemLabel (sample(?geo) as ?geo)  WHERE {
  ?item p:P31/p:P279* ?statement .
  ?statement ps:P31 wd:Q42744322.
  optional { ?item wdt:P625 ?geo . }
SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
} group by ?item ?itemLabel""";
