<https://www.wikidata.org#query-d8e5a08782d4678a5ac74bb2fb623f68> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org/wiki/User:PAC2/D%C3%A9partements>;
