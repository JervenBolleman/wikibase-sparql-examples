  ?class wdt:P279 wd:Q22865 .
  ?city wdt:P31 ?class .
  ?class wdt:P131 ?state .
  ?city wdt:P439 ?key .
  ?city p:P1082 ?pop .
  ?pop v:P1082 ?population .
  ?pop wikibase:rank wikibase:PreferredRank .
  ?pop pq:P585 ?popdate
  SERVICE wikibase:label{bd:serviceParam wikibase:language \"de\"}
}
