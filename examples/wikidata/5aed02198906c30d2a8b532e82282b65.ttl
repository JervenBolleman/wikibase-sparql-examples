<https://www.wikidata.org/#query-5aed02198906c30d2a8b532e82282b65> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """#title: Lexèmes décrivant une couleur
# Auteur: Vesihiisi

SELECT ?l ?lemma ?languageLabel WHERE {
  ?l dct:language ?language;
     wikibase:lemma ?lemma;
     wdt:P31 wd:Q376431.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],mul,en\". }
}
ORDER BY ?languageLabel""";
