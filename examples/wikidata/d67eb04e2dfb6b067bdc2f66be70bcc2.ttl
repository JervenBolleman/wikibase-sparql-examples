  ?item rdfs:label ?lemma.
  ?item schema:description ?desc.	
  FILTER(LANG(?desc) = LANG(?lemma)).
  
  # Filter out common false-positives
  FILTER NOT EXISTS { ?item wdt:P131 ?_ }.
  FILTER NOT EXISTS {
    VALUES ?not { 
    wd:Q4167410 
