  ?nagari wdt:P131 ?kec.
  ?kec wdt:P131 ?kabkota.  
  ?kabkota wdt:P131 wd:Q2772.
  OPTIONAL { ?masjid wdt:P571 ?sejak. }
  	BIND(YEAR(?sejak) as ?year)
  OPTIONAL {
    ?article schema:about ?masjid . 
