  BIND (REPLACE (?name, \"^.*<([\\\\S\\\\s]+\\\\d+[\\\\S\\\\s]*)-([\\\\S\\\\s]*).*$\", \"$1\") AS ?first)
  BIND (REPLACE (?name, \"^.*<([\\\\S\\\\s]+\\\\d+[\\\\S\\\\s]*)-([\\\\S\\\\s]*).*$\", \"$2\") AS ?second)
}""";
