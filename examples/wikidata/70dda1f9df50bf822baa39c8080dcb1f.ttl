<https://www.wikidata.org#query-70dda1f9df50bf822baa39c8080dcb1f> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org/wiki/User:Molarus/SPARQL>;
