  sh:select """PREFIX schema: <http://schema.org/>

SELECT ?q ?desc WHERE {
    ?q schema:description ?desc .
    FILTER (STRSTARTS(?desc, 'Langue'))     # Could also use CONTAINS()
    filter (lang(?desc) = \"fr\") .
}""";
