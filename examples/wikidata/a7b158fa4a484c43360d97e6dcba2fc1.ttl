  sh:prefixes <https://example.org/to_decide/wikidata_prefixes>;
  sh:select """PREFIX schema: <http://schema.org/>
SELECT ?project (count(?item) as ?count) WHERE {
  ?sitelink schema:about ?item ; schema:isPartOf ?project  .
} 
group by ?project 
order by ?count
LIMIT 3000""";
