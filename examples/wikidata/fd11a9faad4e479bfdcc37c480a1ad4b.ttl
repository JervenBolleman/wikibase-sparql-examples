<https://www.wikidata.org/#query-fd11a9faad4e479bfdcc37c480a1ad4b> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:PAC2/Gender_neutral_labels>;
