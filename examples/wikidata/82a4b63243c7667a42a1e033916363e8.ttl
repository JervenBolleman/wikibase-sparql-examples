<https://www.wikidata.org/#query-82a4b63243c7667a42a1e033916363e8> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Jarekt/queries>;
